{
  "name": "who-farted",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Remotes": { "$path": "src/remotes" },
      "Shared": { "$path": "src/shared" }
    },

    "ServerScriptService": {
      "Server": { "$path": "src/server" }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": { "$path": "src/client" }
      }
    },

    "Workspace": {
      "$properties": { "FilteringEnabled": true },
      "Map": { "$path": "maps/School" }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}